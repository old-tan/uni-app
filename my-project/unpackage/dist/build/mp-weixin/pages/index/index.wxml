<view class="home"><swiper indicator-dots circular indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorActiveColor}}"><block wx:for="{{banners}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.src}}"></image></swiper-item></block></swiper><view class="nav"><block wx:for="{{navs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['navs','',index,'path']]]]]]]}}" class="nav-item" bindtap="__e"><view class="img_box"><image src="{{item.iconPath}}"></image></view><text>{{item.title}}</text></view></block></view><view class="news"><view class="news_title">文明快讯<label class="_span"></label></view><view class="news_list"><block wx:for="{{news}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="news_item"><view class="news_img"><image src="{{item.src}}"></image></view><view class="txt"><view>{{item.desc}}</view><text>{{item.time}}</text></view></view></block></view></view><view data-event-opts="{{[['tap',[['join',['$event']]]]]}}" class="fab" bindtap="__e"></view></view>